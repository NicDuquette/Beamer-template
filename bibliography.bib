	Journal = {Journal of Policy Analysis and Management},
	Number = {3},
	Pages = {602-629},
	Title = {Effects of Financial Incentives on Saving Outcomes and Material Well-Being: Evidence From a Randomized Controlled Trial in {Uganda}},
