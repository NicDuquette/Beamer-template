@article{Cascio:2013aa,
	Abstract = {The Voting Rights Act of 1965, called one of the most effective pieces of civil rights legislation in {U.S.} history, generated dramatic increases in black voter registration across the South. We ask whether the increase in black voting rights was accompanied by an increase in blacks' share of public spending. We exploit a key provision of the Act---removal of literacy tests at registration---for identification.  Employing a triple-difference framework over a twenty-year period, we find that counties with higher black population shares in former literacy test states saw greater increases in both voter turnout and state transfers than comparison counties in non-literacy test states, a finding that is consistent with models of distributive politics.},
	Author = {Cascio, Elizabeth U. and Washington, Ebonya},
	Date-Added = {2019-04-13 10:10:05 -0700},
