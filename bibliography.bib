	Journal = {Communications in Statistics - Theory and Methods},
	Number = {21},
	Pages = {10897-10902},
	Publisher = {Taylor & Francis},
	Title = {The Frisch--Waugh--Lovell theorem for the lasso and the ridge regression},
	Url = {https://doi.org/10.1080/03610926.2016.1252403},
	Volume = {46},
	Year = {2017},
